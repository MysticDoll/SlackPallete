{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "node": true,
    "amd": true,
    "es6": true
  },
  "globals": {
    "console": true
  },
  "rules":{
    "semi": 2,
    "eqeqeq": 1,
    "no-console": 0,
    "quotes": 1,
    "react/jsx-uses-vars": 1
  },
  plugins: [
    "react"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true,
      "jsx": true
    }
  }
}
